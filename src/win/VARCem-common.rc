        MENUITEM STR_4087,IDM_SAVE
	MENUSEPARATOR
        MENUITEM STR_4088,IDM_SCREENSHOT
    END

    POPUP STR_HELP
    BEGIN
        MENUITEM STR_4091,IDM_ABOUT
        MENUITEM STR_LOCALIZE,IDM_LOCALIZE
    END
END

